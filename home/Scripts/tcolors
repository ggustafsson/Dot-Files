#!/usr/bin/env zsh

# Written by GÃ¶ran Gustafsson <gustafsson.g@gmail.com>.

# Description: This script prints out the different terminal colors.

# Usage: tcolors

for X in {30..37} ; do
  echo -n "\033[0;${X}m0;${X}m\033[0m "
done

echo

for X in {30..37} ; do
  echo -n "\033[1;${X}m1;${X}m\033[0m "
done

echo

for X in {40..47} ; do
  echo -n "\033[1;${X}m1;${X}m\033[0m "
done

echo

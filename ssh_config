# I use this so the connection will die by itself when my laptop goes to sleep.
# Otherwise I will have to kill the SSH client by hand when I return.
ServerAliveInterval 30
ServerAliveCountMax 2

Host fulknark.se
  HostName fulknark.se
  Port 443

Host hoth
  HostName 192.168.1.2
  Port 443

Host tatooine
  HostName 192.168.1.3

Host dagobah
  HostName 192.168.1.4
